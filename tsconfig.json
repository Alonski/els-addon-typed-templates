{
    "compilerOptions": {
        "pretty": true,
        "lib": ["ES2019", "DOM"],
        "target": "es6",
        "module": "commonjs",
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "composite": true,
        "noEmitOnError": true,

        "strictNullChecks": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "strictPropertyInitialization": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,

        "skipLibCheck": true,

        "alwaysStrict": false,
        "preserveConstEnums": true,
        "newLine": "crlf",

        "types": []
    }
}